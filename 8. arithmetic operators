int main(){


   //arithmetic operators


   /*
   + addition
   - subtraction
   * multiplication
   / division    ---> 분모의 int를 float 으로 바꿔줘야함
   % modulus    ---> gives you the remander of the division
   ++ increment
   -- decrement
   */


   int x = 5;
   int y = 2;


   //float z = x / (float) y;


   x++;
   y--;


   printf("%d\n", y);


   return 0;
}
